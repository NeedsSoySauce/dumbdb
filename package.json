{
  "name": "@needssoysauce/dumbdb",
  "version": "0.1.4",
  "description": "Dumb database that is dumb.",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeedsSoySauce/dumbdb.git"
  },
  "main": "build/index.js",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "build": "webpack --mode=production --node-env=production",
    "pub": "npm publish dist --access public",
    "lint": "eslint \"src/**\" --quiet --fix",
    "lint-ci": "eslint \"src/**\" --quiet",
    "test": "echo Specify a test script in package.json to run tests",
    "prettier": "prettier -w src",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/types": "^9.0.0",
    "async-mutex": "^0.4.0",
    "dotenv": "^16.0.3",
    "octokit": "^2.0.13"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^16.0.0",
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@types/node": "^18.11.18",
    "@types/pino": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@webpack-cli/generators": "^3.0.1",
    "buffer": "^5.7.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "prettier": "2.8.3",
    "rimraf": "^4.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts,.js",
    "ignore": [],
    "exec": "npm run prettier && npm run lint && ts-node ./src/index.ts"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}